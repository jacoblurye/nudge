{
  "short_name": "Nudge",
  "name": "Nudge",
  "description": "Stay focused, you goof!",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "React Ext"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "run_on": "document_start"
    }
  ],
  "permissions": ["tabs", "storage"],
  "version": "1.0"
}
